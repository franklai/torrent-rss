{
  "dependencies": {
    "got": "^11.8.2",
    "he": "^1.2.0",
    "rss": "^1.2.2"
  },
  "scripts": {
    "build": "npx tsc",
    "rss": "node ./built/index.js",
    "lint": "npx eslint ./src",
    "watch": "npx tsc --watch"
  },
  "devDependencies": {
    "@types/he": "^1.1.1",
    "@types/node": "^14.14.31",
    "@types/rss": "^0.0.28",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unicorn": "^28.0.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": [
      "typescript",
      "typescript/prettier",
      "plugin:unicorn/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
